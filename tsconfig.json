{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"experimentalDecorators": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"noImplicitAny": false,
		"pretty": true,
		"lib": ["es5", "dom", "es2015.promise"],
		"types": ["vue-typescript-import-dts"]
	},
	"exclude": [
		"node_modules", "test"
	],
	"atom": {
		"rewriteTsconfig": false
	},
	"compileOnSave": false
}
